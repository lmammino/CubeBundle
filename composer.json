{
    "name": "lmammino/cube-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Bundle to integrate cube-php",
    "keywords": ["Cube", "Storage", "Metrics", "Events", "Analisys"],
    "license": "MIT",
    "authors": [
        {
            "name": "Luciano Mammino",
            "email": "lmammino@oryzone.com"
        }
    ],    
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.1",
        "showclix/cube-php": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": { "Cube\\Bundle\\CubeBundle": "" }
    },
    "target-dir": "Cube/Bundle/CubeBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
